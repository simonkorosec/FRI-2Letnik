typ int : integer;
var a : arr[5] arr [5] int;

fun main(i:int) : int = (
	{for x=0,5,1 : ini(x)},
	{for x=0,5,1 : izpis(x)},	0
) {where var x:int};

fun ini(i:int) : int = (
	{for x=0,5,1 : {a[i][x] = (x + 5*i +1)}}, 0
) {where var x:int};

fun izpis(i:int) : int = (
	{for x=0,5,1 : putInt(a[i][x])}, 0
) {where var x:int}
